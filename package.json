{
	"name": "robert",
	"displayName": "Robert",
	"description": "VSCode extension project",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "robert.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "biome check --write . && biome format --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.2",
		"@types/node": "^24.3.0",
		"@types/vscode": "^1.103.0",
		"typescript": "^5.9.2"
	}
}
