{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true
	},
	"include": ["src/webview/**/*", "src/types/**/*"],
	"exclude": ["node_modules", "out", "**/*.test.*"]
}
